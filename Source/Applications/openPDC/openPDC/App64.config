      <add name="NodeID" value="e7a5235d-cb6f-4864-a96e-a8686f36e599"
        description="Unique Node ID" encrypted="false" />
      <add name="ConnectionString" value="Data Source=serverName; Initial Catalog=openPDC; User Id=username; Password=password"
