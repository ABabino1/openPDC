      <add name="Odbc.DataProviderString" value="AssemblyName={System.Data, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089};ConnectionType=System.Data.Odbc.OdbcConnection;AdapterType=System.Data.Odbc.OdbcDataAdapter" description="Example ODBC database .NET provider string" encrypted="false"/>
    </exampleConnectionSettings>
    <serviceHelper>
      <add name="MonitorServiceHealth" value="True" description="True if the service health is to be monitored; otherwise False."
        encrypted="false" />
      <add name="AllowedRemoteUsers" value="*" description="Comma or semicolon delimited list of user logins allowed to connect to the service remotely."
        encrypted="false" />
      <add name="ImpersonateRemoteUser" value="False" description="True to execute remote commands under the identity of the remote user; otherwise False."
        encrypted="false" />
      <add name="LogStatusUpdates" value="True" description="True if status update messages are to be logged to a text file; otherwise False."
        encrypted="false" />
      <add name="MaxStatusUpdatesLength" value="262144" description="Maximum numbers of characters allowed in update status messages without getting suppressed from being displayed."
        encrypted="false" />
      <add name="MaxStatusUpdatesFrequency" value="100" description="Maximum number of status update messages that can be issued in a second without getting suppressed from being displayed."
        encrypted="false" />
      <add name="RequestHistoryLimit" value="50" description="Number of client request entries to be kept in the history."
        encrypted="false" />
      <add name="SupportTelnetSessions" value="False" description="True to enable the support for remote telnet-like sessions; otherwise False."
        encrypted="false" />
